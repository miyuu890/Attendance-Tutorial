<%= form_with(model: @base, url:bases_create_path,local: true) do |f| %>
  <%=f.label :base_number, "拠点番号", class: "label--edit" %>
  <%= f.number_field :base_number, class: "form-control" %>
  
  <%=f.label :base_name, "拠点名", class: "label--edit" %>
  <%= f.text_field :base_name, class: "form-control" %>
  
  <%=f.label :base_type, "拠点種類", class: "label--edit" %>
  <%= f.text_field :base_type, class: "form-control" %>
  
  <%= f.submit class: "btn btn-primary btn-block" %>
<% end %>  
  
  