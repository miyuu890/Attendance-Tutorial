<h1>出勤社員一覧</h1>

<table class="table table-bordered ">
  <tr>
    <th>社員番号</th>
    <th>名前</th>
  </tr>
     
 <% @user.each do |user| %>
  <% user.attendances.each do |attendance| %>
   <% if (Date.current == attendance.worked_on) && attendance.started_at != nil && attendance.finished_at == nil %>
   <tr>
     <td><%= user.emploee_number %></td>
     <td><%= user.name %></td>
   </tr>
   <% end %>
  <% end %>
 <% end %>  
  
  
  
</table>



  